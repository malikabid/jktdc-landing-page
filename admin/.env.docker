# DOTK Admin - Docker Environment Configuration

# Application
APP_ENV=development
APP_DEBUG=true
APP_URL=http://localhost:8080/admin
APP_VERSION=1.0.0

# Database (Docker MySQL Container)
DB_CONNECTION=mysql
DB_HOST=mysql
DB_PORT=3306
DB_DATABASE=dotk_admin
DB_USERNAME=dotk_admin_user
DB_PASSWORD=dotk_admin_pass

# JWT Configuration
JWT_SECRET=change_this_secret_key_for_production_use_random_64_chars
JWT_ALGORITHM=HS256
JWT_EXPIRATION=3600

# Two-Factor Authentication
TWO_FACTOR_ISSUER="DOTK Admin"
TWO_FACTOR_DIGITS=6
TWO_FACTOR_PERIOD=30
