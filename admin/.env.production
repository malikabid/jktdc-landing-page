# Application
APP_ENV=production
APP_DEBUG=false
APP_URL=https://yourdomain.com
APP_VERSION=1.0.0

# Database - UPDATE THESE WITH YOUR GODADDY DB CREDENTIALS
DB_CONNECTION=mysql
DB_HOST=localhost
DB_PORT=3306
DB_DATABASE=your_database_name
DB_USERNAME=your_db_username
DB_PASSWORD=your_db_password

# JWT Authentication - GENERATE A SECURE KEY
JWT_SECRET=CHANGE-THIS-TO-A-SECURE-RANDOM-STRING-MINIMUM-32-CHARACTERS
JWT_ALGORITHM=HS256
JWT_EXPIRATION=3600

# 2FA
TWO_FACTOR_ISSUER="DOTK Admin"

# Logging
LOG_LEVEL=warning
LOG_PATH=../storage/logs/app.log

# CORS
CORS_ALLOWED_ORIGINS=https://yourdomain.com
